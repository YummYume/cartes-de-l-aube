{
  "type": "module",
  "name": "api",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "ENV='test' vitest run",
    "start": "fastify start -l error app.js",
    "dev": "fastify start -l debug -a 0.0.0.0 -d -I 9320 -w -P app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write .",
    "list:routes": "fastify print-routes app.js",
    "sync:operators": "node ./commands/sync-operators.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^4.3.0",
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^10.1.1",
    "@fastify/jwt": "^7.0.0",
    "@fastify/multipart": "^7.6.1",
    "@fastify/sensible": "^5.0.0",
    "bcrypt": "^5.1.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.0.0",
    "fastify-auth": "^1.2.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "fastify-socket.io": "^4.0.0",
    "mongoose": "^7.2.2",
    "mysql2": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.2",
    "typeorm": "^0.3.16",
    "typeorm-extension": "^2.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "0.2.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tap": "^16.1.0",
    "vitest": "^0.33.0"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "check-coverage": false
  }
}
